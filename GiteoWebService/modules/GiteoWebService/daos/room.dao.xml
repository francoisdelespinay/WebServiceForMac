        <property name="roo_nbNightsMin2" fieldname="roo_nbNightsMin2" datatype="tinyint" required="true"/>
        <property name="roo_cre" fieldname="roo_cre" datatype="datetime" required="true"/>
        <property name="roo_mod" fieldname="roo_mod" datatype="datetime" default=""/>
        <property name="roo_del" fieldname="roo_del" datatype="datetime" default=""/>
        <property name="id_domain" fieldname="id_domain" datatype="int" required="true"/>
