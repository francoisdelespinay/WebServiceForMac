        <property name="cit_urlName" fieldname="cit_urlName" datatype="varchar" required="true" maxlength="100"/>
        <property name="cit_lat" fieldname="cit_lat" datatype="decimal" default=""/>
        <property name="cit_lng" fieldname="cit_lng" datatype="decimal" default=""/>
        <property name="cit_nbCottageSrch" fieldname="cit_nbCottageSrch" datatype="int" required="true"/>
        <property name="cit_nbBabSrch" fieldname="cit_nbBabSrch" datatype="int" required="true"/>
        <property name="id_department" fieldname="id_department" datatype="int" required="true"/>
